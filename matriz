package exemploarras;

import java.util.Scanner;

/**
 *
 * @author internet
 */
public class Exemplo3 {

    public static void main(String[] args) {
        String lista[][] = new String[3][2];
        lista[0][0] = "Jo√£o";
        lista[0][1] = "amoedo presidente";
        lista[1][0] = "Maria";
        lista[1][1] = "Assistente";
        lista[2][0] = "Daniel";
        lista[2][1] = "Faxineiro";
        
        for(int i=0;i<lista.length;i++){
            for(int j=0;j<lista[0].length;j++){
                System.out.println(lista[i][j]);
            }
        }
    }

}
